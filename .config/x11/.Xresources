!! URxvt Appearance
!URxvt.font: xft:Inconsolata:size=8
!URxvt.font: xft:Mono:pixelsize=14:antialias=true
URxvt.font: xft:JetBrainsMono Nerd Font:pixelsize=14:antialias=true
URxvt.boldFont: xft:JetBrainsMono Nerd Font:style=Bold:pixelsize=14:antialias=true
URxvt.italicFont: xft:JetBrainsMono Nerd Font:style=Italic:pixelsize=14:antialias=true
URxvt.boldItalicFont: xft:JetBrainsMono Nerd Font:pixelsize=14:antialias=true
!URxvt.boldFont: xft:
!URxvt.italicFont: xft:DejaVu Sans Mono:pixelsize=15,style=Book
!URxvt.boldItalicfont: xft:DejaVu Sans Mono:pixelsize=15,style=Book
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
!URxvt.internalBorder: 24
URxvt.internalBorder: 4
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 32
URxvt.iso14755: false
! URxvt.background: [90]#1d1f21
URxvt.transparent: true
URxvt.shading: 20

!! URxvt Functions
URxvt.perl-ext-common: resize-font,keyboard-select,url-select
! URxvt.keysym.Shift-Control-V:    perl:pasta:paste
! Comment this if you dont want copy when text is selected
URxvt.clipboard.autocopy: true
! URxvt.resize-font
URxvt.keysym.C-j: resize-font:bigger
URxvt.keysym.C-k: resize-font:smaller
URxvt.keysym.C-equal: resize-font:reset
! URxvt.clickable URLs
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.url-select.button: 1
! URXvt yankable URLs
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
! URxvt keyboard-select
URxvt.keysym.M-p: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
