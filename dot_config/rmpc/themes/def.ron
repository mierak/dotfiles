        marker: "\u{e0b0}",
        ellipsis: "‚Ä¶",
        song_style: None,
        dir_style: None,
    ),
    progress_bar: (
        symbols: ["ÓÇ∂", "‚ñà", "ÓÇ¥", "‚ñà", "ÓÇ¥" ],
        track_style: (fg: "#1e2030"),
        elapsed_style: (fg: "blue"),
        thumb_style: (fg: "blue", bg: "#1e2030"),
    ),
    scrollbar: (
        symbols: ["‚îÇ", "‚ñà", "‚ñ≤", "‚ñº"],
        track_style: (),
        ends_style: (),
        thumb_style: (fg: "blue"),
    ),
    browser_column_widths: [20, 38, 42],
